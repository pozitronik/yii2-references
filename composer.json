{
	"name": "pozitronik/yii2-references",
	"description": "Key-value references model for Yii2",
	"type": "yii2-module",
	"keywords": [
		"yii2",
		"extension",
		"references"
	],
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=8.0",
		"pozitronik/yii2-traits": "^1.0.0",
		"pozitronik/yii2-badgewidget": "^2.0.0",
		"kartik-v/yii2-widget-colorinput": "^1.0.6",
		"kartik-v/yii2-grid": "^3.5.0",
		"kartik-v/yii2-widget-select2": "^2.2.3",
		"kartik-v/yii2-widget-depdrop": "^1.0.6",
		"kartik-v/yii2-bootstrap4-dropdown": "^1.0.4"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master",
		"yiisoft/yii2-bootstrap4": "~2.0.10"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\references\\": "src"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
